<% include ./partials/header %>

<div class="ui main text container segment">
  <div class="ui huge header">Edit Project</div>
  <form class="ui form" action="/projects/<%= project._id %>?_method=PUT" method="POST">
    <div class="field">
      <label>Image</label>
      <input type="text" name="project[image]" placeholder="image" value="<%= project.image %>"></input>
    </div>
    <div class="field">
      <label>Tech List</label>
      <input type="text" name="project[techList]" placeholder="tech list" value="<%= project.techList %>"></input>
    </div>
  <input class="ui blue inverted big button" type="submit"></input>
</form>
</div>


<% include ./partials/footer %>